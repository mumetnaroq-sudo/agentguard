{
  "version": 2,
  "name": "agentguard",
  "builds": [
    {
      "src": "web-dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/subscriptions/pricing",
      "dest": "/api/subscriptions/index.js"
    },
    {
      "src": "/api/subscriptions",
      "dest": "/api/subscriptions/index.js"
    },
    {
      "src": "/api/subscriptions/(.*)",
      "dest": "/api/subscriptions/index.js"
    },
    {
      "src": "/api/webhooks/stripe",
      "dest": "/api/webhooks/stripe.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/billing",
      "dest": "/web-dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web-dashboard/$1"
    }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key",
    "STRIPE_WEBHOOK_SECRET": "@stripe_webhook_secret",
    "STRIPE_PRICE_PRO": "@stripe_price_pro",
    "STRIPE_PRICE_TEAM": "@stripe_price_team",
    "STRIPE_PRICE_ENTERPRISE": "@stripe_price_enterprise",
    "APP_URL": "https://agentguard.vercel.app"
  }
}